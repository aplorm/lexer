{
    "name": "orm/lexer",
    "description": "Lexer for orm",
    "type": "library",
    "require": {
        "orm/common": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan-symfony": "^0.12.4",
        "phpunit/phpunit": "^9.0"
    },
    "license": "mit",
    "authors": [
        {
            "name": "Nicolas Moral",
            "email": "nicolas.m@live.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orm\\Lexer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orm\\Lexer\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Common"
        }
    ]
}
